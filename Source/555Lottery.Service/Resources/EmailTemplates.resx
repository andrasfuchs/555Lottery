  <data name="TEST_BODY" xml:space="preserve">
    <value>&lt;html style="height: 100%; display: table; margin: auto;"&gt;
&lt;head&gt;
    &lt;title&gt;Draw report - @Model.Draw.DrawCode&lt;/title&gt;
&lt;/head&gt;
&lt;body style="height: 100%; display: table-cell; vertical-align: middle; background-color: #000000; color: #8dd9ff;"&gt;
    &lt;div style="width: 960px; z-index: 50;"&gt;
        &lt;div id="biglogo" style="display: block; margin: 44px 100px 0px 100px; width: 780px; height: 156px;"&gt;
            &lt;img runat="server" src="http://beta.555lottery.com/Content/images/555_lottery_logo.png" alt="555 lottery" width="775" height="153" /&gt;
        &lt;/div&gt;
    &lt;/div&gt;
    &lt;div style="color: #8dd9ff; float: none; margin-top: -50px; margin-bottom: 50px; padding: 50px; width: 860px; min-height: 1000px; background: rgba(16, 16, 16, 0.5); border: 3px solid rgba(141, 217, 255, 0.5); border-radius: 10px;"&gt;
        &lt;h1 style="text-align: center"&gt;@Model.Draw.DrawCode&lt;/h1&gt;
        &lt;p&gt;&amp;nbsp;&lt;/p&gt;
        
        &lt;table style="width: 860px; text-align: center; border-width: 1px; border-style: solid; border-color: rgba(141, 217, 255, 0.8); border-radius: 5px; color: #8dd9ff;"&gt;
            &lt;thead style="color: #000000; background-color: #8dd9ff;"&gt;
                &lt;tr&gt;
                    &lt;th&gt;Created&lt;/th&gt;
                    &lt;th&gt;Sequence&lt;/th&gt;
                    &lt;th&gt;Sequence Hash&lt;/th&gt;
                &lt;/tr&gt;
            &lt;/thead&gt;
            &lt;tbody&gt;
                &lt;tr style="background-color: rgba(141, 217, 255, 0.2);"&gt;
                    &lt;td&gt;@Model.Draw.WinningTicketGeneratedAt.Value.ToString("yyyy-MM-dd HH:mm")&lt;/td&gt;
                    &lt;td&gt;@Model.Draw.WinningTicketSequence&lt;/td&gt;
                    @{ string drawHashCheckUrl = "http://beta.555lottery.com/CheckHash/" + Model.Draw.WinningTicketSequence + "/" + BitConverter.ToString(Model.Draw.WinningTicketHash); }
                    &lt;td&gt;&lt;a style="color: #3da9ff; text-decoration: none" href="@drawHashCheckUrl"&gt;check&lt;/a&gt;&lt;/td&gt;
                &lt;/tr&gt;
            &lt;/tbody&gt;
        &lt;/table&gt;

        &lt;p&gt;&amp;nbsp;&lt;/p&gt;

        &lt;table style="width: 860px; text-align: center; border-width: 1px; border-style: solid; border-color: rgba(141, 217, 255, 0.8); border-radius: 5px; color: #8dd9ff;"&gt;
            &lt;thead style="color: #000000; background-color: #8dd9ff;"&gt;
                &lt;tr&gt;
                    &lt;th colspan="3"&gt;Jackpot&lt;/th&gt;
                &lt;/tr&gt;
            &lt;/thead&gt;
            &lt;tbody&gt;
                &lt;tr style="background-color: rgba(141, 217, 255, 0.2);"&gt;
                    &lt;td&gt;@Model.Draw.JackpotBTC BTC&lt;/td&gt;
                    &lt;td&gt;@((Model.Draw.JackpotBTC * Model.Draw.ExchangeRateUSDAtDeadline.Rate).ToString("#,##0.00")) USD&lt;/td&gt;
                    &lt;td&gt;@((Model.Draw.JackpotBTC * Model.Draw.ExchangeRateEURAtDeadline.Rate).ToString("#,##0.00")) EUR&lt;/td&gt;
                &lt;/tr&gt;
            &lt;/tbody&gt;
        &lt;/table&gt;

        &lt;p&gt;&amp;nbsp;&lt;/p&gt;

        &lt;table style="width: 860px; text-align: center; border-width: 1px; border-style: solid; border-color: rgba(141, 217, 255, 0.8); border-radius: 5px; color: #8dd9ff;"&gt;
            &lt;caption&gt;Ticket Lots&lt;/caption&gt;
            &lt;thead style="color: #000000; background-color: #8dd9ff;"&gt;
                &lt;tr&gt;
                    &lt;th&gt;Code&lt;/th&gt;
                    &lt;th&gt;State&lt;/th&gt;
                    &lt;th&gt;TxCreated&lt;/th&gt;
                    &lt;th&gt;TxDownloaded&lt;/th&gt;
                    &lt;th&gt;TxConfirmations&lt;/th&gt;
                    &lt;th&gt;Winnings&lt;/th&gt;
                    &lt;th&gt;Payout&lt;/th&gt;
                &lt;/tr&gt;
            &lt;/thead&gt;
            &lt;tbody&gt;

                @foreach (_555Lottery.DataModel.TicketLot tl in Model.TicketLots)
                { 
                    &lt;tr style="background-color: rgba(141, 217, 255, 0.2);"&gt;
                        &lt;td&gt;@tl.Code&lt;/td&gt;
                        &lt;td&gt;@tl.State&lt;/td&gt;

                        @if (tl.MostRecentTransactionLog != null)
                        { 
                            &lt;td&gt;@tl.MostRecentTransactionLog.BlockTimeStampUtc.ToString("yyyy-MM-dd HH:mm")&lt;/td&gt;
                            &lt;td&gt;@tl.MostRecentTransactionLog.DownloadedUtc.ToString("yyyy-MM-dd HH:mm")&lt;/td&gt;
                            &lt;td&gt;@tl.MostRecentTransactionLog.Confirmations&lt;/td&gt;
                        }
                        else
                        {
                            &lt;td&gt;&lt;/td&gt;
                            &lt;td&gt;&lt;/td&gt;
                            &lt;td&gt;&lt;/td&gt;
                        }

                        @if (tl.WinningsBTC.HasValue)
                        { 
                        &lt;td&gt;@tl.WinningsBTC.Value.ToString("0.00000000")&lt;/td&gt;
                        string payUrl = "bitcoin:" + tl.RefundAddress + "?amount=" + tl.WinningsBTC.Value.ToString("0.00000000") + "&amp;label=555Lottery";
                        &lt;td&gt;&lt;a style="color: #3da9ff; text-decoration: none" href="@payUrl"&gt;pay&lt;/a&gt;&lt;/td&gt;
                        } else {
                            &lt;td&gt;N/A&lt;/td&gt;
                            &lt;td&gt;&lt;/td&gt;
                        }

                    &lt;/tr&gt;
                }
            &lt;/tbody&gt;
        &lt;/table&gt;

        &lt;p&gt;&lt;/p&gt;

        &lt;table style="width: 860px; text-align: center; border-width: 1px; border-style: solid; border-color: rgba(141, 217, 255, 0.8); border-radius: 5px; color: #8dd9ff;"&gt;
            &lt;caption&gt;Valid Games&lt;/caption&gt;
            &lt;thead style="color: #000000; background-color: #8dd9ff;"&gt;
                &lt;tr&gt;
                    &lt;th&gt;Lot Code&lt;/th&gt;
                    &lt;th&gt;Mode&lt;/th&gt;
                    &lt;th&gt;Type&lt;/th&gt;
                    &lt;th&gt;Sequence&lt;/th&gt;
                    &lt;th&gt;Prize&lt;/th&gt;
                    &lt;th&gt;Hash&lt;/th&gt;
                &lt;/tr&gt;
            &lt;/thead&gt;
            &lt;tbody&gt;
                @foreach (_555Lottery.DataModel.Ticket t in Model.ValidTickets)
                {
                    &lt;tr style="background-color: rgba(141, 217, 255, 0.2);"&gt;
                        &lt;td&gt;@t.TicketLot.Code&lt;/td&gt;
                        &lt;td&gt;@t.Mode.ToString()&lt;/td&gt;
                        &lt;td&gt;@t.Type&lt;/td&gt;
                        &lt;td&gt;@t.Sequence&lt;/td&gt;
                        &lt;td&gt;@t.WinningsBTC.Value.ToString("0.00000000")&lt;/td&gt;
                        @{ string hashCheckUrl = "http://beta.555lottery.com/CheckHash/" + t.Sequence + "/" + BitConverter.ToString(t.SequenceHash); }
                        &lt;td&gt;&lt;a style="color: #3da9ff; text-decoration: none" href="@hashCheckUrl"&gt;check&lt;/a&gt;&lt;/td&gt;
                    &lt;/tr&gt;
                }
            &lt;/tbody&gt;
        &lt;/table&gt;

        &lt;p&gt;&lt;/p&gt;

        &lt;table style="width: 860px; text-align: center; border-width: 1px; border-style: solid; border-color: rgba(141, 217, 255, 0.8); border-radius: 5px; color: #8dd9ff;"&gt;
            &lt;caption&gt;Valid Games&lt;/caption&gt;
            &lt;thead style="color: #000000; background-color: #8dd9ff;"&gt;
                &lt;tr&gt;
                    &lt;th&gt;Lot Code&lt;/th&gt;
                    &lt;th&gt;Mode&lt;/th&gt;
                    &lt;th&gt;Sequence&lt;/th&gt;
                    &lt;th&gt;Hits&lt;/th&gt;
                    &lt;th&gt;Prize&lt;/th&gt;
                    &lt;th&gt;Hash&lt;/th&gt;
                &lt;/tr&gt;
            &lt;/thead&gt;
            &lt;tbody&gt;
                @foreach (_555Lottery.DataModel.Game g in Model.ValidGames)
                {
                    &lt;tr style="background-color: rgba(141, 217, 255, 0.2);"&gt;
                        &lt;td&gt;@g.Ticket.TicketLot.Code&lt;/td&gt;
                        &lt;td&gt;@g.Ticket.Mode.ToString()&lt;/td&gt;
                        &lt;td&gt;@g.Sequence&lt;/td&gt;
                        &lt;td&gt;@g.Hits&lt;/td&gt;
                        &lt;td&gt;@g.WinningsBTC.Value.ToString("0.00000000")&lt;/td&gt;
                        @{ string hashCheckUrl = "http://beta.555lottery.com/CheckHash/" + g.Sequence + "/" + BitConverter.ToString(g.SequenceHash); }
                        &lt;td&gt;&lt;a style="color: #3da9ff; text-decoration: none" href="@hashCheckUrl"&gt;check&lt;/a&gt;&lt;/td&gt;
                    &lt;/tr&gt;
                }
            &lt;/tbody&gt;
        &lt;/table&gt;

        &lt;div style="margin-left: 660px;"&gt;
            &lt;p&gt;&amp;nbsp;&lt;/p&gt;
            &lt;p&gt;Have fun,&lt;/p&gt;
            &lt;p&gt;555 Lottery Robot&lt;/p&gt;
        &lt;/div&gt;
    &lt;/div&gt;
&lt;/body&gt;
&lt;/html&gt;</value>
  </data>
  <data name="TEST_SUBJECT" xml:space="preserve">
    <value>Draw report - @Model.Draw.DrawCode</value>
  </data>
