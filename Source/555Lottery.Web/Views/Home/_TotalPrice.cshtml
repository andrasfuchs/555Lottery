@model System.String

@foreach (char c in Model)
{
    string charId = c.ToString();
    charId = charId.Replace(",", "separator").Replace(" ", "space").Replace("|", "line").Replace(".", "dot");
    
    <img runat="server" src="~/Content/images/tbn_@(charId)_blue.png" alt="@(c)" width="10" height="13" />
}